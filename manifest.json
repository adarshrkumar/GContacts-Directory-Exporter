{
    "name": "GContacts Directory Exporter",
    "description": "Exporter for exporting all Directory Items from Google Contacts.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "hello_extensions.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://contacts.google.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.contacts.google.com/"
            ],
            "js": [
                "contacts.js"
            ]
        }
    ]
}